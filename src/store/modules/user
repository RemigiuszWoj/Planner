import userService from "../../services/userService";
const state = { users: [] };
const getters = {};
const actions = {
    fetchUsers({ commit }) {
        return new Promise((resolve, reject) => {
            userService
                .fetchUsers()
                .then((output) => {
                    commit("setUser", output);
                    resolve();
                })
                .catch((err) => {
                    reject(err);
                });
        });
    },
};
const mutations = {
    setUser(state, output) {
        state.users = output;
    },
};
export default { namespaced: true, state, getters, actions, mutations };
